# ftm4 touch
genfscon sysfs /devices/virtual/input/ftm4_touch               u:object_r:sysfs_touch:s0
genfscon sysfs /devices/virtual/input/ftm4_touch/autotune      u:object_r:sysfs_touch_shell:s0

# Symlinks from /sys/class/power_supply
genfscon sysfs /devices/soc/c179000.i2c/i2c-5/5-0049/power_supply u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/soc/c1b5000.i2c/i2c-7/7-0008/c1b5000.i2c:qcom,smb138x@8:qcom,smb138x-parallel-slave@1000/power_supply u:object_r:sysfs_batteryinfo:s0
